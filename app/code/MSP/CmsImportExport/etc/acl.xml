<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="Magento_Backend::content_elements">
                        <resource id="Magento_Cms::page" title="Pages" sortOrder="10">
                            <resource id="MSP_CmsImportExport::export_page" title="Export Page" sortOrder="10" />
                        </resource>
                        <resource id="Magento_Cms::block" title="Blocks" sortOrder="20">
                            <resource id="MSP_CmsImportExport::export_block" title="Export Blocks" sortOrder="10" />
                        </resource>
                        <resource id="MSP_CmsImportExport::import" title="Import" sortOrder="30" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
