<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="attribute_edit_tabs">
            <block class="Mirasvit\LayeredNavigation\Block\Adminhtml\ProductAttribute\Tab\Navigation" name="m_navigation_attribites" />
            <action method="addTabAfter">
                <argument name="name" xsi:type="string">m_navigation_config_section</argument>
                <argument name="block" xsi:type="string">m_navigation_attribites</argument>
                <argument name="after" xsi:type="string">front</argument>
            </action>
        </referenceBlock>
    </body>
</page>
